<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity FicheClient.
    -->
    <changeSet id="20190403192323-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="secteur_activite_id"
                                 baseTableName="fiche_client"
                                 constraintName="fk_fiche_client_secteur_activite_id"
                                 referencedColumnNames="id"
                                 referencedTableName="secteur_activite"/>

        <addForeignKeyConstraint baseColumnNames="activite_id"
                                 baseTableName="fiche_client"
                                 constraintName="fk_fiche_client_activite_id"
                                 referencedColumnNames="id"
                                 referencedTableName="activite"/>

        <addForeignKeyConstraint baseColumnNames="activite_scondaire_id"
                                 baseTableName="fiche_client"
                                 constraintName="fk_fiche_client_activite_scondaire_id"
                                 referencedColumnNames="id"
                                 referencedTableName="activite"/>

        <addForeignKeyConstraint baseColumnNames="region_id"
                                 baseTableName="fiche_client"
                                 constraintName="fk_fiche_client_region_id"
                                 referencedColumnNames="id"
                                 referencedTableName="region"/>

        <addForeignKeyConstraint baseColumnNames="ville_id"
                                 baseTableName="fiche_client"
                                 constraintName="fk_fiche_client_ville_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ville"/>

    </changeSet>
</databaseChangeLog>
